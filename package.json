{
  "name": "vie-xf",
  "version": "1.0.2",
  "description": "VieXF",
  "main": "./out/main/index.js",
  "author": "VieXF",
  "homepage": "https://viex.vercel.app/",
  "scripts": {
    "format": "prettier --write .",
    "start": "set ELECTRON_RUN_AS_NODE=&& electron-vite preview",
    "dev": "set ELECTRON_RUN_AS_NODE=&& electron-vite dev -w",
    "build": "node build.js",
    "protect": "node Backend/scripts/protect.js",
    "build:registry": "npm run build -- --registry",
    "build:electron": "npm run build:vite && electron-builder",
    "build:vite": "set ELECTRON_RUN_AS_NODE=&& electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@fontsource/days-one": "^5.2.7",
    "@headlessui/react": "^2.2.7",
    "@lucide/lab": "^0.1.2",
    "@sentry/electron": "^6.11.0",
    "@sentry/react": "^9.46.0",
    "@tailwindcss/postcss": "^4.1.14",
    "clsx": "^2.1.1",
    "discord-rpc": "^4.0.1",
    "electron-log": "^5.4.3",
    "electron-store": "8.1.0",
    "electron-updater": "^6.6.2",
    "framer-motion": "^12.29.0",
    "i18next": "^25.8.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "lucide-react": "^0.542.0",
    "posthog-js": "^1.261.7",
    "react-i18next": "^16.5.3",
    "react-router-dom": "^7.8.2",
    "react-toastify": "^11.0.5",
    "react-virtuoso": "^4.18.1",
    "recharts": "^3.7.0",
    "systeminformation": "^5.27.8",
    "tailwindcss": "^4.1.14",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^2.1.0",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.7.0",
    "asar": "^3.2.0",
    "electron": "38.0.0",
    "electron-builder": "^26.0.12",
    "electron-vite": "^4.0.0",
    "javascript-obfuscator": "^4.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "vite": "^6.3.5"
  },
  "repository": "https://discord.gg/VRAk33YzkE",
  "build": {
    "publish": [
      {
        "provider": "github",
        "owner": "1hxngz",
        "repo": "VieXF",
        "releaseType": "release"
      }
    ],
    "extraResources": [
      {
        "from": "Backend/resources",
        "to": ".",
        "filter": [
          "*.exe",
          "*.nip",
          "*.pow",
          "*.ico"
        ]
      },
      {
        "from": "Backend/tweaks",
        "to": "tweaks"
      },
      {
        "from": "Backend/scripts",
        "to": "scripts",
        "filter": [
          "**/*",
          "!protect.js"
        ]
      }
    ],
    "appId": "com.viex.app",
    "productName": "VieXF",
    "directories": {
      "buildResources": "build"
    },
    "files": [
      "!**/.vscode/*",
      "!src/*",
      "!Frontend/*",
      "!Backend/*",
      "!dist-old",
      "!dist-2",
      "!electron-builde.yml",
      "!images/*",
      "!electron.vite.config.{js,ts,mjs,cjs}",
      "!{.eslintignore,.eslintrc.cjs,.prettierignore,.prettierrc.yaml,dev-app-update.yml,CHANGELOG.md,README.md}",
      "!{.env,.env.*,.npmrc,pnpm-lock.yaml,yarn.lock}",
      "!docs/**",
      "!eslint.config.mjs",
      "!get.ps1",
      "!license",
      "!trusted-dependencies-scripts.json"
    ],
    "asarUnpack": [
      "resources/**"
    ],
    "win": {
      "executableName": "VieXF",
      "requestedExecutionLevel": "requireAdministrator",
      "icon": "Backend/resources/vie.ico",
      "target": [
        "nsis",
        "zip"
      ]
    },
    "nsis": {
      "artifactName": "${name}-${version}-setup.${ext}",
      "shortcutName": "${productName}",
      "uninstallDisplayName": "${productName}",
      "createDesktopShortcut": "always"
    },
    "npmRebuild": false
  },
  "packageManager": "yarn@1.22.22"
}